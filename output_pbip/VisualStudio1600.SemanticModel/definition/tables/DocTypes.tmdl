	column Reference_number
		dataType: string
		summarizeBy: none
		sourceColumn: Reference_number

	/// Long Invoice Reference Required
	column Rfrnc_requ
		dataType: string
		summarizeBy: none
		sourceColumn: Rfrnc_requ

	/// Indicator: Document type allowed for rollup
	column Rollup
		dataType: string
		summarizeBy: none
		sourceColumn: Rollup

	/// Indicator: Posting from SAP billing document ?
	column SAP_bill_doc
		dataType: string
		summarizeBy: none
		sourceColumn: SAP_bill_doc

	/// ISNULL([T1].[SATZ_ID], 0)
	column DocTypes
		dataType: int64
		isKey
		isNullable: false
		summarizeBy: none
		sourceColumn: SATZ_ID

	/// Self-Issued Document
	column Self-iss
		dataType: string
		summarizeBy: none
		sourceColumn: Self-iss

	/// Document Type
	column Type
		dataType: string
		summarizeBy: none
		sourceColumn: Type

	/// Indicator: Are postings to vendor permitted?
	column Vendor
		dataType: string
		summarizeBy: none
		sourceColumn: Vendor


