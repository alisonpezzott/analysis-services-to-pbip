/// SNAPSHOT
table Snapshot

	column Date
		dataType: dateTime
		summarizeBy: none
		sourceColumn: Date

	column Description
		dataType: string
		summarizeBy: none
		sourceColumn: Description

	column Name
		dataType: string
		isNullable: false
		summarizeBy: none
		sourceColumn: Name

	/// Snapshot ID
	column Snapshot
		dataType: int64
		isKey
		isNullable: false
		summarizeBy: none
		sourceColumn: SATZ_ID


    partition 'Snapshot' = m
        mode: import
        source =
            let
                Source = Sql.Database("DESKTOP-7QLGMPF\LAINOBORGO", "DemoSAP", [Query = "SELECT [Date] AS [Date], [Description] AS [Description], [Name] AS [Name], [SATZ_ID] AS [Snapshot] FROM [STAR].[Snapshot]", CreateNavigationProperties=false])
            in
                Source
    