/// G/L account master (company code)
table Account

	/// Account currency
	column Acct_curr
		dataType: string
		summarizeBy: none
		sourceColumn: Acct_curr

	/// ID for account details
	column Acct_ID
		dataType: string
		summarizeBy: none
		sourceColumn: Acct_ID

	/// Alternative account number in company code
	column Alt_acct
		dataType: string
		summarizeBy: none
		sourceColumn: Alt_acct

	/// Authorization Group
	column Authoriz
		dataType: string
		summarizeBy: none
		sourceColumn: Authoriz

	/// Indicator: Is account only posted to automatically?
	column Auto__posting
		dataType: string
		summarizeBy: none
		sourceColumn: Auto__posting

	/// Cash receipt account / cash disbursement account
	column Cash_flow
		dataType: string
		summarizeBy: none
		sourceColumn: Cash_flow

	/// Accounting clerk
	column Clerk
		dataType: string
		summarizeBy: none
		sourceColumn: Clerk

	/// Client
	column Client
		dataType: string
		summarizeBy: none
		sourceColumn: Client

	/// Commitment Item
	column Cmmt_Item
		dataType: string
		summarizeBy: none
		sourceColumn: Cmmt_Item

	/// Company Code
	column CoCode
		dataType: string
		summarizeBy: none
		sourceColumn: CoCode

	/// Date on which the Record Was Created
	column Creat_date
		dataType: string
		summarizeBy: none
		sourceColumn: Creat_date

	/// Name of Person who Created the Object
	column Created_by
		dataType: string
		summarizeBy: none
		sourceColumn: Created_by

	/// SKB1: End of validity period
	column Account_ValidTo
		dataType: dateTime
		isNullable: false
		summarizeBy: none
		sourceColumn: DAT_BIS_HIST

	/// SKB1: Start of validity period
	column Account_ValidFrom
		dataType: dateTime
		isNullable: false
		summarizeBy: none
		sourceColumn: DAT_VON_HIST

	/// Indicator: Account marked for deletion?
	column Deletion_flag
		dataType: string
		summarizeBy: none
		sourceColumn: Deletion_flag

	/// Key for Exchange Rate Differences in Foreign Currency Accts
	column E_R_difKey
		dataType: string
		summarizeBy: none
		sourceColumn: E_R_difKey

	/// Indicator: account managed in external system
	column Ext_acct
		dataType: string
		summarizeBy: none
		sourceColumn: Ext_acct

	/// Financial Budget Item
	column Fin_bud_im
		dataType: string
		summarizeBy: none
		sourceColumn: Fin_bud_im

	/// Field status group
	column Fld_stat
		dataType: string
		summarizeBy: none
		sourceColumn: Fld_stat

	/// G/L Account Number
	column G_L_acct
		dataType: string
		summarizeBy: none
		sourceColumn: G_L_acct

	/// G/L account additional text
	column G_L_text
		dataType: string
		summarizeBy: none
		sourceColumn: G_L_text

	/// Short Key for a House Bank
	column House_Bank
		dataType: string
		summarizeBy: none
		sourceColumn: House_Bank

	/// Inflation Key
	column Infl__key
		dataType: string
		summarizeBy: none
		sourceColumn: Infl__key

	/// Interest calculation frequency in months
	column Int_cal_fr
		dataType: string
		summarizeBy: none
		sourceColumn: Int_cal_fr

	/// Interest calculation indicator
	column Int_ID
		dataType: string
		summarizeBy: none
		sourceColumn: Int_ID

	/// Indicator: Only Manage Balances in Local Currency
	column LC_bals
		dataType: string
		summarizeBy: none
		sourceColumn: LC_bals

	/// Clearing Specific to Ledger Groups
	column LG-SpecClg
		dataType: string
		summarizeBy: none
		sourceColumn: LG-SpecClg

	/// Indicator: Can Line Items Be Displayed by Account?
	column Line_items
		dataType: string
		summarizeBy: none
		sourceColumn: Line_items

	/// Date of the last interest calculation run
	column Lst_int_rn
		dataType: string
		summarizeBy: none
		sourceColumn: Lst_int_rn

	/// Key date of the last interest calculation
	column Lst_key_dt
		dataType: string
		summarizeBy: none
		sourceColumn: Lst_key_dt

	/// MCA Key
	column MCA__Key
		dataType: string
		summarizeBy: none
		sourceColumn: MCA__Key

	/// Indicator: Open item management?
	column OI_mangmnt
		dataType: string
		summarizeBy: none
		sourceColumn: OI_mangmnt

	/// Planning group
	column Plan_group
		dataType: string
		summarizeBy: none
		sourceColumn: Plan_group

	/// Planning level
	column Plg_level
		dataType: string
		summarizeBy: none
		sourceColumn: Plg_level

	/// Indicator: Is Account Blocked for Posting?
	column Pstg_block
		dataType: string
		summarizeBy: none
		sourceColumn: Pstg_block

	/// Indicator: Reconcil.acct ready for input at time of posting?
	column Re_act_rdy
		dataType: string
		summarizeBy: none
		sourceColumn: Re_act_rdy

	/// Recovery Indicator
	column Rec_Indic
		dataType: string
		summarizeBy: none
		sourceColumn: Rec_Indic

	/// Account is reconciliation account
	column Recon_ID
		dataType: string
		summarizeBy: none
		sourceColumn: Recon_ID

	/// ISNULL([T1].[SATZ_ID], 0)
	column Account
		dataType: int64
		isKey
		isNullable: false
		summarizeBy: none
		sourceColumn: SATZ_ID

	/// Key for sorting according to assignment numbers
	column Sort_key
		dataType: string
		summarizeBy: none
		sourceColumn: Sort_key

	/// Indicator: Supplement for automatic postings ?
	column Supplement
		dataType: string
		summarizeBy: none
		sourceColumn: Supplement

	/// Tax Category in Account Master Record
	column Tax_cat
		dataType: string
		summarizeBy: none
		sourceColumn: Tax_cat

	/// Tolerance Group for G/L Accounts
	column Tol_group
		dataType: string
		summarizeBy: none
		sourceColumn: Tol_group

	/// Valuation Group
	column Valuation_grp
		dataType: string
		summarizeBy: none
		sourceColumn: Valuation_grp

	/// Indicator: Tax code is not a required field
	column W_o_tax
		dataType: string
		summarizeBy: none
		sourceColumn: W_o_tax

	partition DIM_SKB1 = query
		source
			query = SELECT [Client], [CoCode], [G_L_acct], [Acct_curr], [Acct_ID], [Alt_acct], [Authoriz], [Auto__posting], [Cash_flow], [Clerk], [Cmmt_Item], [Creat_date], [Created_by], [DAT_BIS_HIST], [DAT_VON_HIST], [Deletion_flag], [E_R_difKey], [Ext_acct], [Fin_bud_im], [Fld_stat], [G_L_text], [House_Bank], [Infl__key], [Int_cal_fr], [Int_ID], [LC_bals], [LG-SpecClg], [Line_items], [Lst_int_rn], [Lst_key_dt], [MCA__Key], [OI_mangmnt], [Plan_group], [Plg_level], [Pstg_block], [Re_act_rdy], [Rec_Indic], [Recon_ID], [SATZ_ID], [Sort_key], [Supplement], [Tax_cat], [Tol_group], [Valuation_grp], [W_o_tax] FROM [STAR].[DIM_SKB1]
			dataSource: DemoSAP

