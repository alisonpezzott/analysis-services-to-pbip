table Dcoument_Date
	dataCategory: Time

	column CurrentDate
		dataType: int64
		isNullable: false
		summarizeBy: none
		sourceColumn: CurrentDate

	column CurrentMonth
		dataType: int64
		isNullable: false
		summarizeBy: none
		sourceColumn: CurrentMonth

	column CurrentYear
		dataType: int64
		isNullable: false
		summarizeBy: none
		sourceColumn: CurrentYear

	column Date
		dataType: dateTime
		isKey
		dataCategory: Date
		summarizeBy: none
		sourceColumn: Date

	column Day
		dataType: int64
		summarizeBy: none
		sourceColumn: Day

	column ISO_Week
		dataType: int64
		summarizeBy: none
		sourceColumn: ISO_Week

	column LongDate
		dataType: string
		summarizeBy: none
		sourceColumn: LongDate

	column Month
		dataType: int64
		summarizeBy: none
		sourceColumn: Month

	column PrevMonth
		dataType: int64
		isNullable: false
		summarizeBy: none
		sourceColumn: PrevMonth

	/// Calendar ID
	column Calendar
		dataType: int64
		summarizeBy: none
		sourceColumn: SATZ_ID

	column ShortDate
		dataType: string
		summarizeBy: none
		sourceColumn: ShortDate

	column VeryShortDate
		dataType: string
		summarizeBy: none
		sourceColumn: VeryShortDate

	column Week
		dataType: int64
		summarizeBy: none
		sourceColumn: Week

	column Weekday
		dataType: int64
		summarizeBy: none
		sourceColumn: Weekday

	column Year
		dataType: int64
		summarizeBy: none
		sourceColumn: Year

	partition Dcoument_Date = query
		source
			query = SELECT [SATZ_ID], [CurrentDate], [CurrentMonth], [CurrentYear], [Date], [Day], [ISO_Week], [LongDate], [Month], [PrevMonth], [ShortDate], [VeryShortDate], [Week], [Weekday], [Year] FROM [STAR].[Dcoument_Date]
			dataSource: DemoSAP

