table TemporalIntelligence

	calculationGroup

		calculationItem Default = SELECTEDMEASURE()

		calculationItem PY =
				CALCULATE (
				    SELECTEDMEASURE (),
				    SAMEPERIODLASTYEAR ( DimDate[Date] )
				)

	column Name
		dataType: string
		sourceColumn: Name
		sortByColumn: Ordinal

	column Ordinal
		dataType: int64
		isHidden
		sourceColumn: Ordinal


    partition 'TemporalIntelligence' = m
        mode: import
        source =
            let
                Source = Sql.Database("None", "None", [Query = "SELECT [Name] AS [Name], [Ordinal] AS [Ordinal] FROM [dbo].[TemporalIntelligence]", CreateNavigationProperties=false])
            in
                Source
    