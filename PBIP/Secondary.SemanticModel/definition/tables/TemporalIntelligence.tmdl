				    SELECTEDMEASURE (),
				    SAMEPERIODLASTYEAR ( DimDate[Date] )
				)

	column Name
		dataType: string
		sourceColumn: Name
		sortByColumn: Ordinal

	column Ordinal
		dataType: int64
		isHidden
		sourceColumn: Ordinal

